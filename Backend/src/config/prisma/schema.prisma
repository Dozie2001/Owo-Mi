generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id             String @db.Uuid    @id @default(uuid())
  sub            String @unique
  email          String @unique
  provider        Provider @default(Google)
  salt           String
  address        String
}

enum Provider {
  Google 
}
